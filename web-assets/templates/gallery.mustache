<ul id="gallery">
	{{#projects}}
		<li id="{{id}}" class="gallery-item" style="grid-column-start:{{{grid.colStart}}};grid-column-end:span {{{grid.cols}}};grid-row-start:{{{grid.rowStart}}};grid-row-end:span {{{grid.rows}}};background-color:{{backgroundColor}};{{#isPixelArt}}image-rendering:pixelated;{{/isPixelArt}}">
			<a href="{{{uri}}}">
				<div class="gallery-item-image" style="width:{{image.gallery.width}}px;height:{{image.gallery.height}}px;background-image:url('{{{image.gallery.uri}}}');background-size:{{image.gallery.sheetWidth}}px {{image.gallery.sheetHeight}}px;background-position:-{{image.gallery.x}}px -{{image.gallery.y}}px;">
					<span class="screen-reader-only">{{{descriptionStripped}}}</span>
				</div>
			</a>
		</li>
	{{/projects}}
</ul>